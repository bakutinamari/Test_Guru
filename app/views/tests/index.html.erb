<h1>Tests</h1>

<table>
  <tr>
	<th>title</th>
	<th>level</th>
  </tr>

  <% @tests.each do |test| %>
    <tr>
      <td><%= test.title %></td>
      <td><%= test.level %></td>
    </tr>
  <%= end %>
</table>

<div>
  <h2><%= pluralize(@test.questions.count, 'count')%>detected:</h2>
    <ul> 
      <% @test.questions.full_messages.each do |message| %>
        <li><%= message %></li>
          <% end %>
    </ul>
</div>
